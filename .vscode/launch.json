{
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "console": "internalConsole",
      "name": "ng build",
      "skipFiles": [ "<node_internals>/**" ],
      "program": "${workspaceFolder}/node_modules/@angular/cli/bin/ng.js",
      "env": {
        "NODE_PATH": "${workspaceFolder}/node_modules/.pnpm/node_modules"
      },
      "args": [ "build", "--configuration=development" ]
    },
    {
      "type": "node",
      "request": "launch",
      "console": "internalConsole",
      "name": "ng serve",
      "skipFiles": [ "<node_internals>/**" ],
      "program": "${workspaceFolder}/node_modules/@angular/cli/bin/ng.js",
      "env": {
        "NODE_PATH": "${workspaceFolder}/node_modules/.pnpm/node_modules"
      },
      "args": [ "serve" ]
    }
  ]
}
